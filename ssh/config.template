NoHostAuthenticationForLocalhost yes

Host *.github.com github.com
IdentityFile ~/.ssh/git@id_ed25519.github.com
RequestTTY no

Host localhost
StrictHostKeyChecking no
ExitOnForwardFailure yes
ControlMaster no

Host *
ControlMaster yes
ControlPath ~/.ssh/s/%r@%h:%p.sock
ControlPersist yes
CheckHostIP yes
ServerAliveInterval 5
UseRoaming no
ForwardAgent no
HashKnownHosts no
VisualHostKey yes
IdentitiesOnly yes
